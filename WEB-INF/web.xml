<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
  version="4.0"
  metadata-complete="true">

  <display-name>NewContext</display-name>
  <description>
     Welcome to NewContext
  </description>

  <servlet>
    <servlet-name>BoardServletListPoolName</servlet-name>
    <servlet-class>peachy.sv.board.pool.BoardServletListPool</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BoardServletListPoolName</servlet-name>
    <url-pattern>/board_pool/list.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BoardServletInPoolName</servlet-name>
    <servlet-class>peachy.sv.board.pool.BoardServletInPool</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BoardServletInPoolName</servlet-name>
    <url-pattern>/board_pool/in.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BoardServletContPoolName</servlet-name>
    <servlet-class>peachy.sv.board.pool.BoardServletContentPool</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BoardServletContPoolName</servlet-name>
    <url-pattern>/board_pool/content.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BoardServletDelPoolName</servlet-name>
    <servlet-class>peachy.sv.board.pool.BoardServletDelPool</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BoardServletDelPoolName</servlet-name>
    <url-pattern>/board_pool/del.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BoardServletUpPoolName</servlet-name>
    <servlet-class>peachy.sv.board.pool.BoardServletUpdatePool</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BoardServletUpPoolName</servlet-name>
    <url-pattern>/board_pool/update.do</url-pattern>
  </servlet-mapping>
</web-app>